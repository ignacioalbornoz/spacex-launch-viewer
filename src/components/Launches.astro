  method: "POST",
  headers: {
    "Content-Type": "application/json",
  },
  body: JSON.stringify({
    query: {},
    options: {
      sort: {
        date_unix: "desc",
      },
      limit: 12,
    },
  }),
});
const launches = JSON.stringify(await res.json());

console.log(launches);
